{
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "plugins": [
        "node"
    ],
    "rules": {
        "no-console": 0,
        "no-unused-vars": 1
    }
}